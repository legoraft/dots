[
  {
    "bindings": {
      "super-o": "workspace::Open",
      "super-,": "zed::OpenSettings",
      "super-q": "zed::Quit"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "super-x": "editor::Cut",
      "super-c": "editor::Copy",
      "super-v": "editor::Paste",
      "super-z": "editor::Undo",
      "super-shift-z": "editor::Redo",
      "super-a": "editor::SelectAll",
      "super-l": "editor::SelectLine"
    }
  },
  {
    "context": "Editor && mode == full",
    "bindings": {
      "super-f": "buffer_search::Deploy"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "super-.": "terminal_panel::ToggleFocus"
    }
  }
]